{
    "manifest_version": 3,
    "name": "Google Maps Scraper",
    "version": "1.0",
    "description": "Scrapes data from google maps into a table.",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/icon2-removebg-preview.png",
        "48": "/images/icon2-removebg-preview.png",
        "128": "/images/icon2-removebg-preview.png"
      }
    },
    "icons": {
      "16": "/images/icon2-removebg-preview.png",
      "48": "/images/icon2-removebg-preview.png",
      "128": "/images/icon2-removebg-preview.png"
    }
  }
  